<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SubmitReviewServlet</servlet-name>
  	<display-name>SubmitReviewServlet</display-name>
  	<description></description>
  	<servlet-class>com.onlineshop.controller.SubmitReviewServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SubmitComplaintServlet</servlet-name>
  	<display-name>SubmitComplaintServlet</display-name>
  	<description></description>
  	<servlet-class>com.onlineshop.controller.SubmitComplaintServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SubmitReviewServlet</servlet-name>
  	<url-pattern>/SubmitReviewServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SubmitComplaintServlet</servlet-name>
  	<url-pattern>/SubmitComplaintServlet</url-pattern>
  </servlet-mapping>
</web-app>
